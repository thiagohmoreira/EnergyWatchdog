{
    "name": "EnergyWatchdog",
    "description": "This is an OpenComputers / PHP implementation of a controller for reactor and turbines from Big Reactors with capacitors from EnderIO",
    "homepage": "https://github.com/thiagohmoreira/EnergyWatchdog",
    "license": "MIT",
    "keywords": ["EnergyWatchdog", "Minecraft", "OpenComputers", "Big Reactors", "EnderIO"],
    "authors": [
        {
            "name": "Thiago Hummel Moreira",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/thiagohmoreira/EnergyWatchdog/issues",
        "wiki": "https://github.com/thiagohmoreira/EnergyWatchdog/wiki"
    },
    "require": {
        "katzgrau/klogger": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "HostWatchdog\\": "src"
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}
